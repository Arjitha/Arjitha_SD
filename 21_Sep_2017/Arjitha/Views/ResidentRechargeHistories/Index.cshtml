                @Html.DisplayFor(modelItem => item.Tran_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreditRechargeDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RechargeAmount)
            </td>
            <td>

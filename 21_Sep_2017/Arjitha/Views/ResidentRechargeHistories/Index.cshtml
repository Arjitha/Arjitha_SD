@model IEnumerable<Arjitha.Models.ResidentRechargeHistory>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Tran_ID)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CreditRechargeDate)
